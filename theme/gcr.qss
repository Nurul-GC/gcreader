QWidget, QSplashScreen{
    font-family: Copse;
}
QPushButton{
    border-style:solid;
    border-width:1px;
    border-radius:3px;
    border-color:black;
    padding:5px;
}
QPushButton::hover{
    color:gray;
    border-color:gray;
}
QMenuBar{
    color:black;
}
QMenuBar::selected{
    color:gray;
}
QMenu{
    color:black;
}
QMenu::selected{
    color:gray;
}
